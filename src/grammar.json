{
  "name": "dbml",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_definition"
      }
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-z0-9_]+"
    },
    "number": {
      "type": "PATTERN",
      "value": "\\d+"
    },
    "word": {
      "type": "PATTERN",
      "value": "\\w+"
    },
    "text": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": ".+"
        },
        {
          "type": "SYMBOL",
          "name": "_line_break"
        }
      ]
    },
    "string": {
      "type": "PATTERN",
      "value": "[']{1}.*[']{1}"
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "//"
              },
              {
                "type": "PATTERN",
                "value": "[^\\r?\\n]*"
              }
            ]
          },
          {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "/*"
              },
              {
                "type": "PATTERN",
                "value": "[^*]*"
              },
              {
                "type": "STRING",
                "value": "*/"
              }
            ]
          }
        ]
      }
    },
    "_line_break": {
      "type": "PATTERN",
      "value": "[\\?\\r?\\n]"
    },
    "_definition": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "project_definition"
        },
        {
          "type": "SYMBOL",
          "name": "table_definition"
        },
        {
          "type": "SYMBOL",
          "name": "table_group_definition"
        },
        {
          "type": "SYMBOL",
          "name": "enum_definition"
        }
      ]
    },
    "modifiers": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "FIELD",
          "name": "modifier",
          "content": {
            "type": "REPEAT",
            "content": {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "modifier"
                }
              ]
            }
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": ","
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "]"
        }
      ]
    },
    "modifier": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "word"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": ":"
                },
                {
                  "type": "FIELD",
                  "name": "value",
                  "content": {
                    "type": "SYMBOL",
                    "name": "word"
                  }
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "project_definition": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "Project"
        },
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        },
        {
          "type": "SYMBOL",
          "name": "project_block"
        }
      ]
    },
    "project_block": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "keypair"
              },
              {
                "type": "SYMBOL",
                "name": "note_string"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "keypair": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "FIELD",
              "name": "key",
              "content": {
                "type": "SYMBOL",
                "name": "identifier"
              }
            },
            {
              "type": "STRING",
              "value": ":"
            },
            {
              "type": "FIELD",
              "name": "value",
              "content": {
                "type": "SYMBOL",
                "name": "string"
              }
            },
            {
              "type": "SYMBOL",
              "name": "_line_break"
            }
          ]
        }
      ]
    },
    "note_string": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "Note"
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "string"
            },
            {
              "type": "SYMBOL",
              "name": "multi_line_string"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "_line_break"
        }
      ]
    },
    "multi_line_string": {
      "type": "PATTERN",
      "value": "[']{3}[^']+[']{3}"
    },
    "column_definition": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "column_name",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        },
        {
          "type": "FIELD",
          "name": "data_type",
          "content": {
            "type": "SYMBOL",
            "name": "data_type"
          }
        },
        {
          "type": "FIELD",
          "name": "modifiers",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "modifiers"
              },
              {
                "type": "BLANK"
              }
            ]
          }
        },
        {
          "type": "SYMBOL",
          "name": "_line_break"
        }
      ]
    },
    "table_definition": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "Table"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "FIELD",
                  "name": "schema_name",
                  "content": {
                    "type": "SYMBOL",
                    "name": "identifier"
                  }
                },
                {
                  "type": "STRING",
                  "value": "."
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": "as"
                },
                {
                  "type": "FIELD",
                  "name": "alias",
                  "content": {
                    "type": "SYMBOL",
                    "name": "word"
                  }
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "FIELD",
          "name": "definition_block",
          "content": {
            "type": "SYMBOL",
            "name": "block"
          }
        }
      ]
    },
    "table_group_definition": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "TableGroup"
        },
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        },
        {
          "type": "SYMBOL",
          "name": "table_group_block"
        }
      ]
    },
    "table_group_block": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "FIELD",
            "name": "table_name",
            "content": {
              "type": "SYMBOL",
              "name": "text"
            }
          }
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "data_type": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": "("
                },
                {
                  "type": "SYMBOL",
                  "name": "number"
                },
                {
                  "type": "STRING",
                  "value": ")"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "enum_definition": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "Enum"
        },
        {
          "type": "FIELD",
          "name": "enum_name",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        },
        {
          "type": "SYMBOL",
          "name": "enum_block"
        }
      ]
    },
    "enum_block": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "FIELD",
            "name": "constant",
            "content": {
              "type": "SYMBOL",
              "name": "text"
            }
          }
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "block": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "REPEAT",
              "content": {
                "type": "SYMBOL",
                "name": "_statement"
              }
            }
          ]
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "_statement": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "column_definition"
        }
      ]
    },
    "_expression": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "SYMBOL",
          "name": "number"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "SYMBOL",
      "name": "comment"
    },
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

